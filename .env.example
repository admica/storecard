# Database connection string
# For Vercel Postgres, automatic or found in the Storage tab.
# Format: postgres://user:password@host:port/database
DATABASE_URL="postgresql://name:passwd@localhost:5432/mydb?schema=public"
DIRECT_URL="postgresql://name:passwd@localhost:5432/mydb?schema=public"

# NextAuth Secret
# Generate one with: npx auth secret
AUTH_SECRET="your-secret-key"
